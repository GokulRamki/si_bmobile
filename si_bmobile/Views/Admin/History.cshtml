@model List<si_bmobile.Models.User_History>

@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/bm_Layout_Admin.cshtml";
}

<h2>History</h2>

@using (Html.BeginForm("History", "Admin", FormMethod.Post, new { id = "frmHistory", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    <div class="container">
        
        <div class="dRowvlg">
            <div style="text-align: center;">
                @if (ViewBag.Message != null)
                {
                    @ViewBag.Message
                }
            </div> 
        </div>
        <div class="dRowvlg">
            <div class="dAColhead dborder sm">
                Sno
            </div>
            <div class="dAColhead dborder sm">
                UserId
            </div>
            <div class="dAColhead dborder sm">
                BundleId
            </div>
            <div class="dAColhead dborder md">
                Created Date
            </div>
            <div class="dAColhead dborder md">
                Modified Date
            </div>
            
            <div class="clear"></div>
            @if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                <div class="dAColhead dItems sm">
                   
                </div>
                <div class="dAColhead dItems sm">
                    @item.userId
                </div>
                <div class="dAColhead dItems sm">
                    @item.planId
                </div>
                <div class="dAColhead dItems sm">
                    @item.createdDate
                </div>
                <div class="dAColhead dItems sm">
                    @item.modifyDate
                </div>
               
                <div class="clear"></div>
                }
            }
        </div>
    </div>
}